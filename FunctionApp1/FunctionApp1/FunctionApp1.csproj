    <UserSecretsId>8e665599-1947-4e7d-8912-e8aaf3891291</UserSecretsId>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.ApplicationInsights.WorkerService" Version="2.15.0" />
